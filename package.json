  "dependencies": {
    "is-electron": "^2.2.0",
    "react": "16.8.3",
    "react-art": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "0.59.5",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-web_improved": "0.9.9",
    "react-native-windows": "^0.57.1",
    "react-navigation": "^3.9.1",
    "react-redux": "6.0.1",
    "react-router-dom": "4.3.1",
    "react-router-native": "4.3.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "babel-jest": "^23.6.0",
    "babel-plugin-react-native-web": "0.10.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "electron": "4.0.5",
    "electron-packager": "13.1.0",
    "icon-gen": "2.0.0",
    "jest": "^23.6.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-scripts": "2.1.5",
    "react-test-renderer": "16.8.3",
    "rnpm-plugin-dom": "0.5.0",
    "rnpm-plugin-windows": "0.2.8",
    "serve": "10.1.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
